<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <title>Online Visit</title>
    <link href="bootstrap.css" rel="stylesheet" />

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body>
    <nav class="navbar navbar-inverse" role="navigation">
        <ul class="navbar navbar-nav">
            <li style="margin-left:50px;">
                <a href="/main/HtmlPage1">Home</a>
            </li>
            <li style="margin-left:50px;">
                <a href="/AboutUs/AboutUs">About Us</a>
            </li>
            <li style="margin-left:50px;">
                <a href="/Contact/Contact">Contact Us</a>
            </li>
            <li style="margin-left:50px;">
                <a href="/SignIn/SignIn">SignIn</a>
            </li>
            <li style="margin-left:50px;">
                <a href="/SignUp/SignUp">SignUp</a>
            </li>
            <li style="margin-left:50px;">
                <a href="/Admin/AdminPage">Admin</a>
            </li>
        </ul>
        <form class="navbar-form navbar-right" role="search" />
        <div class="form=group">
            <input type="text" class="from-control" placeholder="Search" />
            <button type="submit" class="btn bt-primary">Search</button>
        </div>

    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
