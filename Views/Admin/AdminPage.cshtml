@model webProjectMVC.Models.Admin
@{
    Layout = null;
}
<script>
    function f()
    {
        var x = $(d).val();
        $.getJSON("/Admin/Check?uname="+x, function (data)
        {
           
            if(data)
            {
                $('#d').text("User Already Exist");
            }
            else
            {
                $('#d').text("User AvailAble");
            }
        });
    }
</script>
<form action="/Admin/Save" method="post">
    <input type="text" placeholder="UserName" name="UserName"/><div id="d"></div>
    <button type="button" class="btn btn-default" value="Check" onclick="f()"></button> 
    <input type="password"  placeholder="password" name="Password"/>
    <input type="submit" value="SignIn" />
</form>